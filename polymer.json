  "shell": "src/fb-app.html",
  "fragments": [
    "src/fb-blood.html",
    "src/fb-form.html",
    "src/fb-history.html",
    "src/fb-home.html",
    "src/fb-hospital.html",
    "src/fb-landing.html",
    "src/fb-request.html",
